<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>Student</servlet-name>
        <servlet-class>com.fpt.edu.controller.StudentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Student</servlet-name>
        <url-pattern>/student/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentList</servlet-name>
        <servlet-class>com.fpt.edu.controller.ListStudentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentList</servlet-name>
        <url-pattern>/student/list</url-pattern>
    </servlet-mapping>
    
</web-app>